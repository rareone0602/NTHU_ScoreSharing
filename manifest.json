{
  "manifest_version": 2,
  "name": "NTHU_ScoreSharing",
  "version": "0.908",
  "description": "A community sharing score distributions in NTHU",
  "content_scripts": [
    {
      "matches": ["*://www.ccxp.nthu.edu.tw/ccxp/*"],
      "js" : [
        "send_data.js", "recolor_list.js", "draw_on_syllabus.js",
        "draw_img.js", "driver.js", "third-party/Chart.min.js",
        "third-party/moment.min.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions" : [
    "https://nthuscoresharing.herokuapp.com/"
  ],
  "icons": { "128": "icon128.png" }
}
